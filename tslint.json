{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-react-hooks"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "quotemark": { "severity": "off" },
    "no-implicit-dependencies": [true, ["dev", "@testing-library/react"]],
    "interface-name": false,
    "max-line-length": { "severity": "off" },
    "jsx-no-multiline-js": { "severity": "off" },
    "react-hooks-nesting": "error",
    "arrow-parens": { "severity": "off" },
    "object-literal-sort-keys": { "severity": "off" },
    "no-empty": [true, "allow-empty-functions"],
    "ordered-imports": false
  },
  "rulesDirectory": []
}
