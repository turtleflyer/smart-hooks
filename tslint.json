{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-implicit-dependencies": [
      true,
      [
        "dev",
        "@testing-library/react"
      ]
    ],
    "interface-name": false,
    "max-line-length": [
      true,
      90
    ],
    "jsx-no-multiline-js": false,
    "react-hooks-nesting": "error"
  },
  "rulesDirectory": []
}