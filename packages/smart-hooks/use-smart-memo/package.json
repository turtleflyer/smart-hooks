{
  "name": "@smart-hooks/use-smart-memo",
  "version": "3.0.1",
  "description": "Make useMemo predictable",
  "keywords": [
    "react"
  ],
  "author": "Andrei Kovalev <35089047+turtleflyer@users.noreply.github.com>",
  "homepage": "https://github.com/turtleflyer/smart-hooks/tree/master/packages/smart-hooks/use-smart-memo#readme",
  "license": "Unlicense",
  "main": "lib/use-smart-memo.cjs.js",
  "module": "lib/use-smart-memo.esm.js",
  "browser": "lib/use-smart-memo.umd.js",
  "types": "lib/use-smart-memo.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turtleflyer/smart-hooks.git"
  },
  "scripts": {
    "test:pre": "jest pre",
    "test:post": "jest post",
    "build:js": "tsc -p ./tsconfig.bundle.json --rootDir .",
    "build:types": "tsc -p ./tsconfig.bundle.json --emitDeclarationOnly --declaration --outDir lib",
    "cleanup_lib": "node ../../../bundle_utilities/cleanUpDistFolder.js lib",
    "format:space": "node ../../../bundle_utilities/insertSpaceLines.js lib *.d.ts",
    "format:prettier": "prettier ./lib --write --ignore-path ../../../.prettierignore",
    "bundle": "npm run cleanup_lib && npm run build:js && npm run build:types && rollup -c && npm run format:space && npm run format:prettier"
  },
  "bugs": {
    "url": "https://github.com/turtleflyer/smart-hooks/issues"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "gitHead": "9a47ccf24e6f09b4f17ebf01516e8c4833f2290f"
}
